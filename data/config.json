{
  "framework": "langchain",
  "model_provider": {
    "name": "openai",
    "model": "gpt-4-turbo-preview",
    "credentials": {
      "api_key": "env:OPENAI_API_KEY"
    },
    "parameters": {
      "temperature": 0.7,
      "max_tokens": 4096
    }
  },
  "generation_settings": {
    "max_iterations": 10,
    "enable_testing": true,
    "output_directory": "outputs/",
    "verbose": true,
    "save_progress": true
  }
}
